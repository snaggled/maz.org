<div id="checkins" style="width: 390px; height: 450px;"></div>

<div class="prepend-top">
  <%= link_to(image_tag('poweredby-foursquare.png',
        :alt => '', :size => '150x24'), 'http://foursquare.com/',
        :target => '_new') %>
</div>

<% content_for(:head) do %>
  <title>maz.org</title>
  <%= javascript_include_tag 'date' %>
  <%= content_tag(:script, nil,
        :src => 'http://maps.google.com/maps/api/js?sensor=false') %>
<% end %>

<% content_for(:script) do %>
  $('div#checkins').checkin_map({
    url: '<%= checkins_path %>'
  });
<% end %>
